    "add": "git add .",
    "commit": "czg",
    "prepare": "husky install",
    "serve": "vite",
    "start": "cross-env WEBPACK_DEV_SERVER_URL=true nodemon --exec electron . --watch ./ --ext .js,.html,.scss,.vue,.ts,.css",
